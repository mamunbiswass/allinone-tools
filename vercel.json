{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "framework": "vite",
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "enabled": true,
    "autoJobCancelation": true
  }
}
